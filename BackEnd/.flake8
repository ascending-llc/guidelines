[flake8]
exclude = .svn,CVS,.bzr,.hg,.git,__pycache__,.tox,.eggs,*.egg,build,dist
filename = *.py
ignore = W503,W504
max-line-length = 100
max-complexity = 10
import-order-style = appnexus
; per-file-ignores =
;     PATH/TO/SINGLE/FILE/RELATIVE/TO/PROJECT/FOLDER:ErrorCode
; Example:
; per-file-ignores =
;     src/dal/shadow_order.py:N815